{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/app",
      "rewrite": "/index.html",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    }
  ]
}